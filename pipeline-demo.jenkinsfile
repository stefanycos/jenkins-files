#!groovy

@Library('shared-library-demo')_

node {
    stage('Checkout') {
        echo 'Starting demo pipeline - Checkout'
        gitCheckout (
            branch : 'master',
            url : 'https://github.com/stefanycos/springboot-client.git'
        )
    }
    
     stage('Build') {
            tool name: 'Maven-3.5.0', type: 'maven'
            sh "mvn -DskipTests -U clean package"
     }
}
